{
    "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "lib": ["DOM", "ESNext"],
      "jsx": "react",
      "declaration": true,
      "declarationDir": "dist",
      "outDir": "dist",
      "strict": true,
      "typeRoots": ["node_modules/@types", "node_modules"],
      "types": ["react", "react-dom", "styled-components"],
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src/"]
  }
  